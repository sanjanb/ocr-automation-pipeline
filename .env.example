# AI-Powered OCR Configuration\n# Copy this to .env and fill in your API tokens\n\n# Hugging Face API Token (get from https://huggingface.co/settings/tokens)\nHUGGING_FACE_TOKEN=your_token_here\n\n# AI Extraction Settings\nAI_EXTRACTION_ENABLED=true\nAI_FALLBACK_THRESHOLD=0.6\nAI_REQUEST_TIMEOUT=30\n\n# Model Preferences (optional)\nPREFERRED_AI_MODEL=donut\nMAX_RETRIES=2\n\n# Traditional OCR Settings (fallback)\nOCR_ENGINE_PRIORITY=easyocr,tesseract\nOCR_CONFIDENCE_THRESHOLD=0.5\n\n# Logging\nLOG_LEVEL=INFO\nLOG_AI_RESPONSES=false